<Application x:Class="Hasherer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style Parts/Menu.xaml"/>
                <ResourceDictionary Source="Style Parts/Buttons.xaml"/>
                <ResourceDictionary Source="Style Parts/Lists.xaml"/>
                <ResourceDictionary Source="Style Parts/Scrollbars.xaml"/>
                <ResourceDictionary Source="Style Parts/Templates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
